[Desktop Entry]
Name=SwayFX_env
Comment=An i3-compatible Wayland compositor (with env vars set)
Exec=bash -c "export SDL_VIDEODRIVER=wayland; export _JAVA_AWT_WM_NONREPARENTING=1; export QT_QPA_PLATFORM=wayland; export XDG_CURRENT_DESKTOP=sway:wlroots ; export XDG_SESSION_DESKTOP=sway; sway"
Type=Application
DesktopNames=sway;wlroots
